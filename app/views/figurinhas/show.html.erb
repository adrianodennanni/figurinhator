<div class="page-header">
  <%= link_to figurinhas_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_figurinha_path(@figurinha), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show figurinha</h1>
</div>

<dl class="dl-horizontal">
  <dt>Numero:</dt>
  <dd><%= @figurinha.numero %></dd>

  <dt>Quantidade:</dt>
  <dd><%= @figurinha.quantidade %></dd>

  <dt>User:</dt>
  <dd><%= @figurinha.user %></dd>

</dl>
